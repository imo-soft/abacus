{
  "name": "abacus",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "mobile-detect": "^1.4.5",
    "sweetalert2": "^7.33.1",
    "vue": "^2.6.4",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "webpack": "^4.24.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "plugin:vue/base"
    ],
    "parserOptions": {
      "parser": [
        "@babel/eslint-parser",
        "vue-eslint-parser"
      ]
    },
    "overrides": {
      "files": ["src/views/**/*.vue"],
      "rules": {
          "vue/multi-word-component-names": 0,
          "no-mixed-spaces-and-tabs": 0
      }
    },
    "rules": {
      "quotes": ["error", "double"]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
